
<h1>All Figures</h1>
<ul>
    <%@figures.each do |f|%>
        <li>
        <label>Name: <%=f.name%></label>
        <label>Title:
            <%if !f.titles.empty?%>
                <%= f.titles.last.name%>
            <%else%>
                <%="No Title"%>
            <%end%>
        </label>
        </li>
    <%end%>

</ul>